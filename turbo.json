{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build", "type-check", "lint"],
      "outputs": ["build/*"]
    }
  }
}
